
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'mojialin_ipv6'
	option use_ipv6 '1'
	option service_name 'dynv6.com'
	option domain 'mojialin.dynv6.net'
	option lookup_host 'mojialin.dynv6.net'
	option password 'Xzs6qafN6eP95kbY_wcG1yZyP3xusT'
	option force_unit 'minutes'
	option use_syslog '0'
	option use_logfile '0'
	option check_interval '5'
	option force_interval '5'
	option ip_source 'interface'
	option interface 'LAN6'
	option ip_network 'LAN6'
	option enabled '0'

config service 'mojialin_ip4'
	option service_name 'dynv6.com'
	option lookup_host 'mojialin.dynv6.net'
	option enabled '1'
	option use_ipv6 '0'
	option domain 'mojialin.dynv6.net'
	option username 'mojialin99@qq.com'
	option password 'Xzs6qafN6eP95kbY_wcG1yZyP3xusT'
	option ip_source 'interface'
	option ip_interface 'wan'
	option interface 'wan'
	option use_syslog '2'
	option check_interval '5'
	option check_unit 'minutes'
	option force_interval '5'
	option force_unit 'minutes'
	option retry_unit 'seconds'

config service 'moyulong_goip_de'
	option service_name 'goip.de'
	option password 'OKHswlrl5FsH'
	option check_interval '5'
	option force_interval '5'
	option force_unit 'minutes'
	option use_syslog '0'
	option use_logfile '0'
	option domain 'moyulong.goip.de'
	option username 'mojialin99@qq.com'
	option lookup_host 'moyulong.goip.de'
	option enabled '0'

